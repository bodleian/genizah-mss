                    <msContents>                            
                        <msItem> 
                            <msItem n="1" xml:id="MS_Heb_g_5_01">
                                <locus from="1a" to="8b"/>
                                <author/>
                                <title>הושענא</title>
                                <note/>
                                <msItem n="a"><p>beg. missing, end <quote type="end">שֿערי תשובה יפתחו לנו. תֿמיד יומם ולילה לא יסגרו</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="b"><p>(<locus from="1b" to="1b">fol. 1b</locus>) beg. <quote type="beg">תתרומם לער במלכות עוזך</quote>, (ת̇ש̇ר̇ק̇);</p></msItem> 
                                <msItem n="c"><p>(<locus from="2a" to="2a">fol. 2</locus>) <quote type="beg">ענה אֿום א̇ עצובת רוח</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="d"><p>(<locus from="2b" to="4a">fol. 2b</locus>) <quote type="beg">אנה אֿל נא הושענה</quote>;</p></msItem> 
                                <msItem n="e"><p>(<locus from="4b" to="4b">fol. 4b</locus>) <quote type="beg">ה̇ ותתפרקון ותישתזבון מן חרבא ומן שביתא</quote>;</p></msItem> 
                                <msItem n="f"><p>(<locus from="5a" to="5a">fol. 5</locus>) <quote type="beg">אדון האדונים. בוחן כליות</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="g"><p>(<locus from="5b" to="5b">fol. 5b</locus>) <quote type="beg">ה̇ למ̇ אֿמוני בריתך</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="h"><p>(<locus from="6a" to="6a">fol. 6</locus>) <quote type="beg">אבן שתייה. בית הבחירה</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="i"><p>(<locus from="6b" to="6b">fol. 6b</locus>) <quote type="beg">אל למושעות. בארבע שבועות</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="k"><p>(<locus from="6b" to="7a">ibid.</locus>) <quote type="beg">הנותן תשועה הושיעה עמך</quote>, (ת̇ש̇ר̇ק̇);</p></msItem> 
                                <msItem n="l"><p>(<locus from="7b" to="7b">fol. 7b</locus>) <quote type="beg">אתה תקום להושיענו. קומה יי</quote>, (א̇ב̇ to י);</p></msItem> 
                                <msItem n="m"><p>(<locus from="7b" to="8b">ibid.</locus>) <quote type="beg">קול מבשר ... אומר לציון</quote>, (א̇ב̇ to ל).</p></msItem>
                                
                                
                                
                            </msItem>
                            <msItem n="2" xml:id="MS_Heb_g_5_02">
                                <locus from="9a" to="15a">Fol. 9</locus>
                                <msItem>
                                    <p>headed <quote type="head">שבעה לכפור</quote>, beg. <quote type="beg">אם יוקש עונינו תסלח</quote>, followed by:</p>
                                </msItem>
                                <msItem>
                                    <msItem n="a"><p>(<locus from="10a" to="10a">fol. 10</locus>), <quote type="beg">אקראך ותענה. בצפצוף מענה</quote>, (א̇ב̇);</p></msItem>
                                    <msItem n="b"><p>(<locus from="10b" to="13a">fol. 10b</locus>) beg. <quote type="beg">אתן תהילה לאל המהולל</quote>, (א̇ב̇);</p></msItem>
                                    <msItem n="c"><p>(<locus from="13b" to="15a">fol. 13b</locus>) beg. <quote type="beg">ארחותיכם אישרתי. דלתותיכם כישרתי. שובו ואשובה</quote>, (א̇ב̇ to כ), ends with common prayer <quote type="beg">ובכן מה נאמר</quote>.</p></msItem>
                                </msItem>
                                
                            </msItem>
                            <msItem n="3" xml:id="MS_Heb_g_5_03">
                                <locus from="15b" to="35b"/>
                                <author/>
                                
                                <note/>
                                <msItem n="a"><p>(<locus from="15b" to="16b">fol. 15b</locus>) headed <quote type="head">זה זולת לוירא בלק</quote>, beg. <quote type="beg">אז לישראל הייתי כאב</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="b"><p>(<locus from="17a" to="17b">fol. 17</locus>) headed <quote type="head">יוצר פינחס</quote>, beg. <quote type="beg">אות ברית כהניי</quote>, (א̇ב̇), and <quote type="head">זולת</quote> (<locus from="18a" to="18b">fol. 18</locus>) beg. <quote type="beg">אתה אלוה השלום</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="c"><p>(<locus from="19a" to="19b">fol. 19</locus>) headed <quote type="head">יוצר אלה מסעי</quote>, beg. <quote type="beg">אתו יי מארץ אדם</quote>, (א̇ב̇), followed by <quote type="head">אופן</quote> (<locus from="20a" to="21a">fol. 20</locus>) and <quote type="head">זולת</quote>, beg. <quote type="beg">ארבעים ושתי מסעות</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="d"><p>(<locus from="21b" to="23a">fol. 21b</locus>) headed <quote type="head">יוצר אלה הדברים</quote>, beg. <quote type="beg">אומן הנקראים חבר</quote>, (א̇ב̇), </p>
                                    <p>followed by <quote type="head">אופן</quote>, (<locus from="23b" to="23b">fol. 23b</locus> <quote type="head">הדא תמם אופן אלה הדברים</quote>), and <quote type="head">זולת</quote> (<locus from="24a" to="24b">fol. 24</locus>), beg. <quote type="beg">אשר המתברכים בארץ</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="e"><p>(<locus from="25a" to="25b">fol. 25</locus>) <quote type="head">יוצר ואתחנן</quote>, beg. <quote type="beg">אמר עניו באימרי עיניני</quote>, (א̇ב̇);</p> 
                                    <p><locus from="26a" to="26b">fol.26</locus>, <quote type="head">זולת</quote>, beg. <quote type="beg">אהוביך הוצאתי</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="f"><p>(<locus from="27a" to="27b">fol. 27</locus>) <quote type="head">יוצר והיה עקב</quote>, beg. <quote type="beg">אמונתי למען תדעון</quote>, (א̇ב̇);</p> 
                                    <p><locus from="28a" to="28a">fol. 28</locus>, <quote type="head">זולת</quote>, beg. <quote type="beg">אל מושך חסד</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="g"><p>(<locus from="28b" to="29b">fol.28b</locus>) <quote type="head">יוצר ראה</quote>, beg. <quote type="beg">אור מאירת עיניכם</quote>, (א̇ב̇);</p> 
                                    <p><locus from="30a" to="31a">fol. 30</locus>, <quote type="head">זולת</quote>, beg. <quote type="beg">אימצתה בדברך</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="h"><p>(<locus from="31b" to="32b">fol. 31b</locus>) <quote type="head">יוצר שופטים</quote>, beg. <quote type="beg">אז בבואך לארץ חבלך</quote>, (א̇ב̇);</p> 
                                    <p><locus from="33a" to="33a">fol. 33</locus>, <quote type="head">זולת</quote>, beg. <quote type="beg">אמת מארץ תצמח</quote>, (א̇ב̇);</p></msItem> 
                                <msItem n="i"><p>(<locus from="33b" to="34b">fol. 33b</locus>) <quote type="head">יוצר כי תצא</quote>, beg. <quote type="beg">אלהים איש מלחמה</quote>, (א̇ב̇);</p> 
                                    <p><locus from="35a" to="35b">fol. 35</locus>, <quote type="head">זולת</quote>, beg. <quote type="beg">ארשנו טעמינו</quote>, (א̇ב̇); end obliterated.</p></msItem>
                                
                                
                                
                                
                            </msItem>
                            <textLang mainLang="heb" otherLangs="jrb"/>
                        </msItem>  
                    </msContents>
