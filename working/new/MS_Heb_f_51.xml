                                <msItem n="c"><locus from="8a">(fol. 8)</locus> <title>Exodus xviii. 16 to xix. 16</title></msItem>
                                <msItem n="d"><locus from="9a">(fol. 9)</locus> <title>Exodus xxvi. 31 to xxviii. 3</title></msItem>
                                
