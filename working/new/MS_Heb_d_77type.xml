                    <msPart n="6" xml:id="MS_Heb_d_77_06">
                        <altIdentifier type="partial">
                            <idno>MS. Heb. d. 77/6</idno>
                        </altIdentifier>
                        <msContents>                            
                            <msItem>
                                <locus from="7a" to="7b">Fol. 7</locus>
                                <author/>
                                <title>Liturgical</title>
                                <note/>  
                                <msItem n="1">
                                    <p>headed <quote type="head">שבעתה</quote> beg. <quote type="beg">ב̇ה̇ש̇ את שמחתי הכפיל</quote>;</p>
                                </msItem>
                                <msItem n="2">
                                    <p><quote type="head">יוצר</quote> beg. <quote type="beg">אתן? סועא? בעמוד קהלכם</quote>;</p>
                                </msItem>
                                <msItem n="3">
                                    <p><quote type="head">יוצר</quote> beg. <quote type="beg">אתה גלית אוזן עמך אל נורא ואיום</quote>;</p>
                                </msItem>
                                <msItem n="4">
                                    <p><locus from="7b" to="7b">Verso</locus>, headed <quote type="head">יום השמיני</quote> beg. <quote type="beg">אמוני מ...עוטרים עטרת</quote>;</p>
                                </msItem>
                                <msItem n="5">
                                    <p>beg. <quote type="beg">אראלי מעונים</quote>;</p>
                                </msItem>
                                <msItem n="6">
                                    <p><quote type="head">מאורות</quote> beg. <quote type="beg">יום ראשון שעבר</quote>;</p>
                                </msItem>
                                <msItem n="7">
                                    <p><quote type="head">יוצר</quote>, same beg., the rest missing, acr?;</p>
                                </msItem>
                                <msItem n="8">
                                    <p><quote type="head">זולתך</quote> beg. <quote type="beg">אזעת כהתקבצו כל קהל?</quote>;</p>
                                </msItem>
                                <msItem n="9">
                                    <p>headed <quote type="head">אחר</quote> beg. <quote type="beg">אנשי ... מקרא? ברוך</quote>.</p>
                                </msItem>
                                <textLang mainLang="heb"/>
                            </msItem>
                             
                        </msContents>
                        <physDesc>
                            <p>Yemen Rabb. char.; <material>paper</material>, probably parts of two leaves originally about 7 1/2 x 5 1/2 in., 2 columns, 39 lines; much injured and partly illegible.</p>
                        </physDesc>
