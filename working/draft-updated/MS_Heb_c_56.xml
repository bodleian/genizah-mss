                  <msContents>
                     <msItem xml:id="MS_Heb_c_56-part3-item1">
                        <locus from="3a" to="3b">Fol. 3</locus>
                        <title>Accounts; Letter</title>
                        <msItem xml:id="MS_Heb_c_56-part3-item1-item1">
                           <locus from="3a" to="3b">Recto</locus>
                           <title>Accounts in Arabic</title>
                           <note>beg.<quote type="beg">ברכאת י̇נ̇ תיא ת... טור... ונץ אלצ... אלמבארך יום אלג̇</quote>.</note>
                        </msItem>
                        <msItem xml:id="MS_Heb_c_56-part3-item1-item2">
                           <locus from="3b" to="3b">Verso</locus>
                           <title>A letter in Arabic</title>
                           <note>'Your letter from<placeName>Tripoli</placeName>reached me, and another letter from<persName>Mahdiah</persName>. I am glad you are well'. Beginning and end ? missing.</note>
                        </msItem>
                        <textLang mainLang="jrb" otherLangs="ar"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>
                        <locus from="3a" to="3a">Recto</locus>, or. curs. char.;<locus from="3b" to="3b">verso</locus>Arabic Naskhi (careless);<material>paper</material>, 9 x 3 3/4 in., stained and injured.</p>
                  </physDesc>
                  <history>
                     <acquisition>Given in<date calendar="#Gregorian" when="1910">1910</date>by<persName>A. Cowley</persName>.</acquisition>
                  </history>
