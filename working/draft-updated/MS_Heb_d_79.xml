               <msPart xml:id="MS_Heb_d_79-part1" n="1">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 79/1</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_79-part1-item1">
                        <locus from="1a" to="4b">Fols. 1-4</locus>
                        <title>Gen. 37,9 to 43,2</title>
                        <note>with vowel points and accents.</note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Span. squ. char.;<material>vellum</material>, 9 1/2 x 7 in., 2 columns, 24 lines, stained and slightly injured.</p>
                  </physDesc>
                  <history>
                     <acquisition>From the Genizah, given in<date calendar="#Gregorian" when="1910">1910</date>by<persName>A. Cowley</persName>.</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_79-part2" n="2">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 79/2</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_79-part2-item1">
                        <locus from="5a" to="6b">Fols. 5, 6</locus>
                        <title>Exod. 14,19 to 15,15 and 25,19 to 26,5.</title>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Quire mark ח on<locus from="5a" to="5b">fol. 5</locus>.</p>
                     <p>Syr. ? squ. char.;<material>vellum</material>, 10 1/2 x 8 1/2 in., 2 columns (except the song), 24 lines. Stained and faded. The 1st column has been inked over again.</p>
                  </physDesc>
                  <history>
                     <acquisition>From the Genizah, given in<date calendar="#Gregorian" when="1910">1910</date>by<persName>A. Cowley</persName>.</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_79-part3" n="3">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 79/3</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_79-part3-item1">
                        <locus from="7a" to="7b">Fol. 7</locus>
                        <title>Lev. 4,2-34</title>
                        <note>with vowel points and accents.</note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Span. squ. char.;<material>vellum</material>, 9 1/2 x 7 1/4 in., 2 columns, 25 lines.</p>
                  </physDesc>
                  <history>
                     <acquisition>From the Genizah, given in<date calendar="#Gregorian" when="1910">1910</date>by<persName>A. Cowley</persName>.</acquisition>
                  </history>
