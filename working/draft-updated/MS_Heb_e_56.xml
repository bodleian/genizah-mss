                                <author/>
                                <title>Magic; book of Enoch</title>
                                <note/>  
                              
                                <msItem xml:id="MS_Heb_e_56-part3-item1-item1" n="a">
                           <locus from="40a">(fol. 40)</locus> 
                           <title>fragment of a magical work on angels</title>
                        </msItem>
                                <msItem xml:id="MS_Heb_e_56-part3-item1-item2" n="b">
                           <locus from="41a">(fol. 41)</locus> 
                           <title>headed <quote type="head">זה ספר לחנוך בן ירד</quote>
                           </title> 
                           <note>incomplete.</note>
                        </msItem>
                               
                              
                                <textLang mainLang="he"/>
                            </msItem>
                             
