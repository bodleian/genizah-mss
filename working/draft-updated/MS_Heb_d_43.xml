                     <msItem xml:id="MS_Heb_d_43-part17-item1">
                        <locus from="46a" to="49b">Fol. 46</locus>
                        <title>A fragment of<persName type="aut">Seadiah</persName>'s Arabic introduction to Job</title>
                        <note>followed by the commentary on i. 1 to 17 (prepared for publication).</note>
                        <textLang mainLang="jrb"/>
