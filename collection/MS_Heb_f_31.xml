                    </msPart>
                    <msPart n="4" xml:id="MS_Heb_f_31_04">
                        <altIdentifier type="partial">
                            <idno>MS. Heb. f. 31/4</idno>
                        </altIdentifier>
                        <msContents>                            
                            <msItem>
                                 <locus from="58a" to="59b"/>
                                <author/>
                                <title>Small liturgical pieces</title>
                                <note/>  
                                <msItem n="a">
                                    <p>(<locus from="58a" to="58b">fol. 58</locus>) beg. <quote type="beg">נורא המאיר לי אפילתי</quote>, headed <quote type="head">וזן יום לריבי</quote>, numbered ב̇;</p>
                                </msItem>
                                <msItem n="b">
                                    <p>(<locus from="59a" to="59b">fol. 59</locus>, ג̇) beg. <quote type="beg">ייי לקראתי חסדיך הקרה</quote>, headed <quote type="head">וזן</quote>.</p>
                                </msItem>
                                <note>With vowel points.</note>
                                <textLang mainLang="heb" otherLangs="jrb"/>
                            </msItem>
                             
                        </msContents>
                        <physDesc>
                            <p>Syr. Rabb. char.; 8vo, <material>paper</material>.</p>
                        </physDesc>
                        <history>
                            <acquisition>Bought through the Rev. <persName>G. J. Chester</persName>, <date calendar="Gregorian" when="1891">1891</date> (from the Genizah). 
                            </acquisition>
                        </history>
                        
                    </msPart>
                    <msPart n="5" xml:id="MS_Heb_f_31_05">
                        <altIdentifier type="partial">
                            <idno>MS. Heb. f. 31/5</idno>
                        </altIdentifier>
                        <msContents>                            
                            <msItem>
                                <locus from="60a" to="61b">Fol. 60</locus>
                                <author/>
                                <title>Liturgical</title>
                                <note/>  
                                <msItem n="a">
                                    <p>beg. missing, end <quote type="end">שובו בנים שובבים</quote> (<quote type="head">פזמון</quote>);</p>
                                </msItem>
                                <msItem n="b">
                                    <p>(<locus from="60b" to="61a">fol. 60b</locus>) headed <quote type="head">אחרת ליהודה הלוי ז̇צ̇ל̇</quote>, beg. <quote type="beg">יד מעללי תכבד כבלי</quote>, acr. <quote type="acr">יהודה</quote>;</p>
                                </msItem>
                                <msItem n="c">
                                    <p>(<locus from="61b" to="61b">fol. 61b</locus>) beg. <quote type="beg">אם עונות יענו בנו</quote>.</p>
                                </msItem>
                                <textLang mainLang="heb"/>
                            </msItem>
                             
                        </msContents>
                        <physDesc>
                            <p>Syr. Rabb. char.; 8vo, <material>paper</material>.</p>
                        </physDesc>
                        <history>
                            <acquisition>Bought through the Rev. <persName>G. J. Chester</persName>, <date calendar="Gregorian" when="1891">1891</date> (from the Genizah). 
                            </acquisition>
                        </history>
                        
                    </msPart>
                    <msPart n="6" xml:id="MS_Heb_f_31_06">
                        <altIdentifier type="partial">
                            <idno>MS. Heb. f. 31/6</idno>
                        </altIdentifier>
                        <msContents>                            
                            <msItem>
                                 <locus from="62a" to="70b"/>
                                <author/>
                                <title>Liturgical</title>
                                <note/>
                                    <msItem n="a"><p>(<locus from="62a" to="62a">fol. 62</locus>) beg. missing (?), beg. <quote type="beg">ר̇ הב לחמא לאנשא ועשבא</quote>;</p></msItem> 
                                    <msItem n="b"><p>(<locus from="62b" to="65a">fol. 62b</locus>) beg. <quote type="beg">אהבה. בריתך. גאולה</quote>, (א̇ב̇), followed by <quote type="beg">דעני</quote>, more than in the printed text;</p> 
                                    <p><locus from="65b" to="65b">fol. 65b</locus>, <quote type="beg">אוהביך שוכני עפר</quote>; <quote type="beg">באנשי כנסת הגדולה</quote>;</p> 
                                    <p><locus from="66a" to="67a">fol. 66</locus>, <quote type="beg">למה יאמרו הגוים</quote>; <quote type="beg">אדון הסליחות ... אם לא תאֿריך אפך</quote>;</p> 
                                    <p><locus from="67b" to="67b">fol. 67b</locus>, another <quote type="beg">דעני</quote>;</p> 
                                    <p><locus from="68a" to="69a">fol. 68</locus>, <quote type="beg">ר̇ אֿיתרצי למילי פומן</quote>; <quote type="beg">למען אֿיש צדיק</quote>;</p> 
                                    <p><locus from="69b" to="69a">fol. 69b</locus>, <quote type="beg">אלהי אל תאֿבד</quote>;</p> 
                                    <p><locus from="70a" to="70b">fol. 70</locus>, <quote type="beg">ר̇ חטאן רח̇ אסכילנא</quote>, incomplete.</p></msItem>
                                    
                                    
                                 
                                
                                <textLang mainLang="heb" otherLangs="arc"/>
                            </msItem>
                             
                        </msContents>
