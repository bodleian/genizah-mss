               <msPart xml:id="MS_Heb_d_55-part4" n="4">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 55/4</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_55-part4-item1">
                        <locus from="17a" to="20b"/>
                        <title>Liturgical fragments</title>
                        <msItem xml:id="MS_Heb_d_55-part4-item1-item1" n="a">
                           <p>(<locus from="17b">fol. 17b</locus>) headed<quote type="head">[פזמ]ון למנחה</quote>, beg.<quote type="beg">..ם שלשה חיננו עדיך</quote>, (א̇ב̇ to ח), acr.<quote type="acr">יוסי</quote>(?);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part4-item1-item2" n="b">
                           <p>(<locus from="18a">fol. 18</locus>) based upon<quote>ויאמרו אל אלהים</quote>, beg.<quote type="beg">הוא היחיד יוצר נגוהים</quote>, acr.<quote type="acr">יוסף מארדי</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part4-item1-item3" n="c">
                           <p>(<locus from="18b">fol. 18b</locus>)<quote type="beg">את אומץ אמונתך אשוחחה</quote>, (א̇ב̇), only two lines left;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part4-item1-item4" n="d">
                           <p>(<locus from="19a">fol. 19</locus>)<quote type="head">ודוי</quote>, beg. missing, beg.<quote type="beg">סררנו לפניך כפרה ס[וררה] ועברנו על ספר ויקרא</quote>, acr.<quote type="acr">[יו]סף הקטן ח... וחזק</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part4-item1-item5" n="e">
                           <p>(<locus from="20b">fol. 20b</locus>) beg.<quote type="beg">ארוממך מלך המרבה סליחה</quote>.</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Greek curs. char.; 8vo,<material>vellum</material>, injured.</p>
                  </physDesc>
                  <history>
                     <acquisition>From the Genizah,<date calendar="#Gregorian" when="1892">1892</date>.</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_55-part5" n="5">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 55/5</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_55-part5-item1">
                        <locus from="21a" to="25b"/>
                        <title>Liturgical</title>
                        <msItem xml:id="MS_Heb_d_55-part5-item1-item1" n="a">
                           <p>(<locus from="21b">fol. 21b</locus>) headed<quote type="head">..גזם חמיש לאלדובר. עשר שבטין נגאלו בבוא דיבר. שומרו וכ̇</quote>, beg.<quote type="beg">אמנם כמעש נבחר מעשרה</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part5-item1-item2" n="b">
                           <p>(<locus from="22b">fol. 22b</locus>) headed<quote type="head">ובכן עשר תעשר</quote>, beg.<quote type="beg">אבות מאן קיימו עשר תעשר</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part5-item1-item3" n="c">
                           <p>(<locus from="23a">fol. 23</locus>) headed<quote type="head">מעריב כי תשא</quote>, beg.<quote type="beg">כי תשא את ראש אמונים בשקליה</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part5-item1-item4" n="d">
                           <p>(<locus from="23b">fol. 23b</locus>) headed<quote type="head">יוצר לזכור</quote>, beg.<quote type="beg">זכור אתיית עמלק בהמצאות בכעס</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part5-item1-item5" n="e">
                           <p>(<locus from="24a">fol. 24</locus>) headed<quote type="head">יוצר לפרה</quote>, beg.<quote type="beg">ארבעה דברים בסתם</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part5-item1-item6" n="f">
                           <p>(<locus from="24b">fol. 24b</locus>) headed<quote type="head">(so) שמונה עשרה לפורה</quote>, beg.<quote type="beg">איש יהודי אֿמיץ בצורו</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part5-item1-item7" n="g">
                           <p>(<locus from="25b">fol. 25b</locus>) headed<quote type="head">אחרת</quote>, beg.<quote type="beg">אבד אלופי אדום</quote>, (א̇ב̇).</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. squ. Rabb. char.; 8vo,<material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>From the Genizah,<date calendar="#Gregorian" when="1892">1892</date>.</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_55-part6" n="6">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 55/6</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_55-part6-item1">
                        <locus from="26a" to="26b">Fol. 26</locus>
                        <title>Liturgical</title>
                        <msItem xml:id="MS_Heb_d_55-part6-item1-item1" n="a">
                           <p>liturgy, א̇ב̇ ends<quote type="end">לאבדינו נֿורא עלילה מסרו בחיינו ו̇ה̇י̇פ̇ל̇נ̇ו̇</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part6-item1-item2" n="b">
                           <p>headed<quote type="head">חורן</quote>, beg. (injured)<quote type="beg">איש חמודות</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part6-item1-item3" n="c">
                           <p>(<locus from="26b">fol. 26b</locus>) headed<quote type="head">חורן</quote>, beg.<quote type="beg">אתה איבדתה חדוהי</quote>.</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. Rabb. char.; 8vo,<material>vellum</material>, injured and stained.</p>
                  </physDesc>
                  <history>
                     <acquisition>From the Genizah,<date calendar="#Gregorian" when="1892">1892</date>.</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_55-part7" n="7">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 55/7</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_55-part7-item1">
                        <locus from="27a" to="30b"/>
                        <title>Liturgical</title>
                        <msItem xml:id="MS_Heb_d_55-part7-item1-item1" n="a">
                           <p>(<locus from="27a">fol. 27</locus>) end of a liturgy concerning the ten martyrs, ends<quote type="end">תופיע ברחמים על עם נברא</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part7-item1-item2" n="b">
                           <p>(<locus from="27a">ibid.</locus>) beg.<quote>אביע כתם</quote>, (א̇ב̇), Zunz, p. 221;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part7-item1-item3" n="c">
                           <p>(<locus from="28b">fol. 28b</locus>) beg.<quote type="beg">אחזה רעדה</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part7-item1-item4" n="d">
                           <p>(<locus from="30a">fol. 30</locus>)<quote type="beg">אנשי אמונה כבודם נדד</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_55-part7-item1-item5" n="e">
                           <p>
                              <quote type="beg">איכה אבלה קרויה בשם</quote>, (א̇ב̇).</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. squ. char.; 8vo,<material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>From the Genizah,<date calendar="#Gregorian" when="1892">1892</date>.</acquisition>
                  </history>
