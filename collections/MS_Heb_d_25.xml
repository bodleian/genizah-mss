               <msPart xml:id="MS_Heb_d_25-part1" n="A">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 25/A</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_25-part1-item1">
                        <locus from="1a" to="1b"/>
                        <title>Liturgical</title>
                        <msItem xml:id="MS_Heb_d_25-part1-item1-item1" n="1">
                           <p>beg. <quote type="beg">יום לריב תעמוד</quote> (פזמון), acr. <quote type="acr">לוי חזק</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part1-item1-item2" n="2">
                           <p>(<locus from="1b" to="1b">fol. 1b</locus>) <quote type="beg">ירצה צור</quote> (פזמון), acr. <quote type="acr">יהודה</quote> (Zunz, p. 699);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part1-item1-item3" n="3">
                           <p>(<locus from="1b" to="1b">ibid.</locus>) <quote type="beg">שוחר טוב</quote>, beg. only (Zunz, p. 190, by <persName>Gabirol</persName>).</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. Rabb. char.; oblong, <material>paper</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName>G. J. Chester</persName>, <date calendar="#Gregorian" when="1890">1890</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_25-part2" n="B">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 25/B</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_25-part2-item1">
                        <locus from="2a" to="9b"/>
                        <title>Lamentations</title>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item1" n="1">
                           <p>beg. missing, end הצמיח ארוכה ועליהו לתרופה;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item2" n="2">
                           <p>(<locus from="2a" to="2a">ibid.</locus>) <quote type="beg">אנה אלך אֿבלה וחפויה ראש</quote> (<persName>Qalir</persName>, Zunz, p. 49);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item3" n="3">
                           <p>(<locus from="2b" to="2b">fol. 2b</locus>) beg. <quote type="beg">מה אֿמרתם ולא אצעק</quote>, end <quote type="end">מה תֿכניתם</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item4" n="4">
                           <p>(<locus from="3a" to="3a">fol. 3</locus>) beg. <quote type="beg">אֿתן מלך הן לצדק ימלך</quote> (<persName>Qalir</persName>, Zunz, p. 49, no. 33), end <quote type="end">קֿבע סמל ינע רציון</quote>, incomplete;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item5" n="5">
                           <p>(<locus from="3b" to="3b">fol. 3b</locus>) <quote type="beg">איה כה</quote> (Zunz, p. 49, no. 27);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item6" n="6">
                           <p>(<locus from="4a" to="4a">fol. 4</locus>) <quote type="beg">אפפוני ארבע מלכיות</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item7" n="7">
                           <p>(<locus from="4a" to="4b">ibid.</locus>) beg. <quote type="beg">נבוכדנצר אֿכלני הממני</quote> end <quote type="end">נבוכדנצר שֿבר קדשנו</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item8" n="8">
                           <p>(<locus from="5a" to="5a">fol. 5</locus>) beg. <quote type="beg">אנה אלך ואבכה</quote>, end <quote type="end">על תֿריסין ביקרא ...</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item9" n="9">
                           <p>(<locus from="5a" to="5a">ibid.</locus>) <quote type="beg">אמרתי בסיני אֿנכי יי̇ אלהיך</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item10" n="10">
                           <p>(<locus from="5b" to="5b">fol. 5b</locus>) <quote type="beg">אללי לי חויבנו לשמע אֿיכה</quote>, incomplete;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item11" n="11">
                           <p>(<locus from="6a" to="6a">fol. 6</locus>) only last strophe, beg. <quote type="end">תני נפשך משמרים לבקר</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item12" n="12">
                           <p>(<locus from="6a" to="6a">ibid.</locus>) beg. <quote type="beg">אנה אלך ואבכה על אֿכלי הקדשים</quote>, end <quote type="end">אנה ... על תֿרומת המעשר</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item13" n="13">
                           <p>(<locus from="6b" to="6b">fol. 6b</locus>) beg. <quote type="beg">אֿנחו קריאי נחלת חבל</quote>, end of every strophe <quote type="end">איך אתנחמה</quote>, end <quote type="end">קלע תֿקלע תשמיע</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item14" n="14">
                           <p>(<locus from="7a" to="7a">fol. 7</locus>) beg. <quote type="beg">איך תנחמוני הבל</quote> (<persName>Qalir</persName>, Zunz, p. 50, no. 35);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item15" n="15">
                           <p>(<locus from="7b" to="7b">fol. 7b</locus>) <quote type="beg">באו אֿרומים בני ארורים</quote>, end <quote type="end">באו שֿקוצים</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item16" n="16">
                           <p>(<locus from="7b" to="7b">ibid.</locus>) <quote type="beg">מנחמינו איך השמיעוני</quote> (<persName>Qalir</persName>, Zunz, p. 49. no. 32);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item17" n="17">
                           <p>(<locus from="8a" to="8a">fol. 8</locus>) <quote type="beg">איה יום אֿותות לילה</quote>, end <quote type="end">איה יום קֿימת משכן ...</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item18" n="18">
                           <p>(<locus from="8b" to="8b">fol. 8b</locus>) <quote type="beg">על אֿרע קדישא על בֿריא רחימא</quote>, end <quote type="end">בעמקיא על שכולין בארעיא</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part2-item1-item19" n="19">
                           <p>(<locus from="9a" to="9b">fol. 9</locus>) <quote type="beg">אכלני הממוני</quote>, incomplete.</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. curs. char.; 8vo, <material>paper</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName>G. J. Chester</persName>, <date calendar="#Gregorian" when="1890">1890</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_25-part3" n="C">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 25/C</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_25-part3-item1">
                        <locus from="10a" to="13b">Fol. 10</locus>
                        <title>Fragment of a סידור</title>
                        <note>beg. with <quote type="beg">לסכוי בינה</quote> to the first <quote>הללויה</quote>, followed (<locus from="12a" to="13b">fol. 12</locus>) by the end of <quote>נשמת</quote> (ישתבח referred to by כֿוֿלֿ) with following remark: <quote>אלי ההונא אנתהת אלזואיד פי אלצלואת אלדי יטלבהא מן אראד אלכמאל</quote>. <p>Next come <quote>שבע ברכות</quote> for שחרית, מוסף, and מנחה followed by other ברכות, headed (<locus from="13b" to="13b">fol. 13b</locus>) <quote type="head">ואגבאת אלטעאם</quote>, incomplete, only a part of ברכת מזון.</p>
                        </note>
                        <textLang mainLang="he" otherLangs="jrb"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. squ. Rabb. char.; 4to, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName>G. J. Chester</persName>, <date calendar="#Gregorian" when="1890">1890</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_25-part4" n="D">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 25/D</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_25-part4-item1">
                        <locus from="14a" to="14b">Fol. 14</locus>
                        <title>Fragment of a סידור</title>
                        <note>containing chiefly the rules and prayers for the Pentateuch lessons, beg. <quote type="beg">אלסדר כל פסוק מרתין עבראני ומרה סוריאני ותכרג אלתוראה</quote>.</note>
                        <textLang mainLang="he" otherLangs="jrb"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. squ. char.; 4to, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName>G. J. Chester</persName>, <date calendar="#Gregorian" when="1890">1890</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_25-part5" n="E">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 25/E</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_25-part5-item1">
                        <locus from="15a" to="16b"/>
                        <title>Liturgical</title>
                        <msItem xml:id="MS_Heb_d_25-part5-item1-item1" n="1">
                           <p>(<locus from="15a" to="15a">fol. 15</locus>) beg. <quote type="beg">אֿל נערץ בסוד קדושים בֿעלונו אדונים קדושים</quote>, at the end the date <date calendar="#Hebrew-Seleucid" when="1335">1335 Sel.</date> = <date calendar="#Gregorian" when="1024">1024 A.D.</date> is given, end <quote type="end">יום יעמד מיכאל השר. לשמים ... למועד מועדים מעלמו גירוש ... לימים אלף שלש מאות חמשה ושלשים</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part5-item1-item2" n="2">
                           <p>(<locus from="15a" to="15b">ibid.</locus>) the first line obliterated, the second (<locus from="15b" to="15b">fol. 15b</locus>), <quote type="beg">אֿחזה רעדה לשבטי תם. בֿעת הישיב האיש אותם</quote>, containing the history of ויגש, incomplete;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part5-item1-item3" n="3">
                           <p>(<locus from="16a" to="16a">fol. 16</locus>) beg. missing, end <quote type="end">תֿל תלפיות קרית חנה</quote>; then followed <quote type="beg">איתן נמתה לו הבט נא השמים</quote>, acr. <quote type="acr">אברהם בן שמימור חזן</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part5-item1-item4" n="4">
                           <p>(<locus from="16a" to="16a">ibid.</locus>) beg. <quote type="beg">בֿעלם אֿרבע מלכיות קמו עלינו</quote>. end <quote type="end">תֿמיד קומה יי̇ יפוצו אויביך</quote>, headed <quote type="beg">רהט לחנכה</quote>; then follows <quote type="head">סילוק</quote>, beg. <quote type="beg">כי אתה אלהי עולם. באותות רזי עולם</quote>, end <quote type="end">ישלשו קדושה בהילולים ק̇ק̇ק̇ יענו כנועם שוכלם</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part5-item1-item5" n="5">
                           <p>(<locus from="16b" to="16b">fol. 16b</locus>) <quote type="head">רהט לחנוכה</quote>, beg. <quote type="beg">כמו געה חנוכת שמים וארץ</quote>, end <quote type="end">ראה סולם בחלום. רגלי מבשר משמיע שלום</quote>.</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. Rabb. char.; 4to, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <origin>
                        <origDate when="1024">1024</origDate>
                     </origin>
                     <acquisition>Bought through the Rev. <persName>G. J. Chester</persName>, <date calendar="#Gregorian" when="1890">1890</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_25-part6" n="F">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 25/F</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_25-part6-item1">
                        <locus from="17a" to="18b">Fol. 17</locus>
                        <title>Fragment of an עבודה, beg. and end missing.</title>
                        <note>The following passages may help indentification: <p>
                              <quote>כבודו אֿמוניו ינקה מאשמתם. בֿעבור שמו הבלול בשמותם. גֿודל מביעים ליוצר נשמתם. דֿובבים תחינה ליודיע מספר שמותם. הֿליל ישמיעו בהוד נעימתם. וֿירצו לצורם בקומתם. זֿמר בסלדם בקול נעימתם. חֿיות יענו לעומתם וברוך יאמרו</quote>;</p>
                           <p>
                              <locus from="18a" to="18b">fol. 18</locus>,</p>
                           <p>
                              <quote>אֿברים מעל. אז יתנו בעקב על אשנב אור היבריק בקדם אתא למיקוה סגן. היגיש עם נתח. על אשוריו. קידש תהילה. בֿעת כי קידש. קרץ תמיד זבח. טרם בעלותו. יקדימנו סגן. בכתונת בד עט. כמו מסויים. טרם בה יכפר. למפשיטי כתונת</quote>.</p>
                           <p>
                              <locus from="17a" to="17b">Fol. 17</locus> is probably not part of an עבודה.</p>
                           <p>
                              <locus from="18a" to="18b">Fol. 18</locus> is part of the same as No. <ref target="MS_Heb_e_25_06">2701 6</ref>.</p>
                           <p>With vowel points.</p>
                        </note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. squ. char.; 4to, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName>G. J. Chester</persName>, <date calendar="#Gregorian" when="1890">1890</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_25-part7" n="G">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 25/G</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_25-part7-item1">
                        <locus from="19a" to="22b">Fol. 19</locus>
                        <title>Fragment of a Mahazor, Spanish rite</title>
                        <note>beg. after פסוקים (beg. missing):</note>
                        <msItem xml:id="MS_Heb_d_25-part7-item1-item1" n="1">
                           <p>
                              <quote type="beg">יענה כבוד אבות</quote> (פזמון), with <quote type="head">כרוג</quote>, beg. <quote type="beg">יושיע צופים</quote>, acr. <quote type="acr">יצחק גיאת</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part7-item1-item2" n="2">
                           <p>
                              <quote type="beg">אבן שתיה בקודש הושתה</quote> (<quote type="head">מחיה</quote>), א̇ב̇, acr. <quote type="acr">יצחק בן יהודה (?) חזק</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part7-item1-item3" n="3">
                           <p>(<locus from="20b" to="20b">fol. 20b</locus>) <quote type="beg">יום צדו צעדי</quote> (פזמון), incomplete;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part7-item1-item4" n="4">
                           <p>(<locus from="21a" to="21a">fol. 21</locus>) beg. missing, last strophe beg. <quote>קרא ישע לגולה ונדה ממעוניו</quote>, acr. <quote type="acr">[יצ]חק</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part7-item1-item5" n="5">
                           <p>(<locus from="21a" to="21b">ibid.</locus>) <quote type="beg">מעשיך אֿין ערוך</quote>, (א̇ב̇, and acr. <quote type="acr">יצחק</quote>), headed <quote type="head">ובכן מעשיך</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part7-item1-item6" n="6">
                           <p>(<locus from="22a" to="22a">fol. 22</locus>) <quote type="beg">עמקו מחשבותיך. ועצמו נפלאותיך</quote> (פזמון);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part7-item1-item7" n="7">
                           <p>(<locus from="22a" to="22b">ibid.</locus>) <quote type="beg">יי̇ אֿבי עד באין נולד</quote>, (א̇ב̇), headed <quote type="head">ובכן יי̇</quote>; ends with the heading <quote type="head">ובכן אבאר שמו בלי כחד</quote>.</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Span. Rabb. char.; 8vo, <material>paper</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName>G. J. Chester</persName>, <date calendar="#Gregorian" when="1890">1890</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_25-part8" n="H">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 25/H</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_25-part8-item1">
                        <locus from="23a" to="24b">Fol. 23</locus>
                        <title>A fragment of the סידור of <persName>Seadiah</persName>.</title>
                        <note>The following is the end of a chapter: <p>
                              <quote>כלא האזינו פאן תפצילהא מרסום מחדוד האזינו. זכר ימות עולם. ירכיבהו על במתי ארץ. וירא יי̇ וינאץ. לו חכמו. כי אשא אל שמ̇. ויבא משה. ואמא אלי̇ פואסיק אלתי ליום אלאתנין ואלכמיס פמשהורה. תם בעון אלרחמן</quote>, as in Soferim.</p>
                           <p>Then follow the introduction of No. 1096, fol. 33, and part of the prayers (Or. Littbl. XII, p. 492).</p>
                        </note>
                        <textLang mainLang="he" otherLangs="jrb"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. squ. Rabb. char.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName>G. J. Chester</persName>, <date calendar="#Gregorian" when="1890">1890</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_25-part9" n="I">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 25/I</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_25-part9-item1">
                        <locus from="25a" to="26b"/>
                        <title>סליחות</title>
                        <msItem xml:id="MS_Heb_d_25-part9-item1-item1" n="1">
                           <p>(<locus from="25a" to="25b">fol. 25</locus>) beg. missing, ג reads <quote>גאה גונן גדועה גנוחה</quote>, (א̇א̇א̇א̇), acr. <quote type="acr">יוסף הק</quote>, end missing;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_25-part9-item1-item2" n="2">
                           <p>(<locus from="26a" to="26b">fol. 26</locus>) beg. after introductory lines, <quote type="beg">אֿשמנו ... לפני נגוהיך</quote>, end <quote type="end">ומרינו לפניך. משים בארץ שמות. ועברנו על ספר ואלה שמות</quote>, headed <quote type="head">חטאנו למנחה</quote>, acr. <quote type="acr">יוסף</quote>?</p>
                        </msItem>
                        <note>With vowel points.</note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. curs. char.; small fol., <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName>G. J. Chester</persName>, <date calendar="#Gregorian" when="1890">1890</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
            </msDesc>
         </sourceDesc>
