               <msPart xml:id="MS_Heb_f_22-part1" n="1">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. f. 22/1</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_f_22-part1-item1">
                        <locus from="1a" to="8b">Fol. 1</locus>
                        <title>Liturgical</title>
                        <msItem xml:id="MS_Heb_f_22-part1-item1-item1" n="a">
                           <p>
                              <quote type="head">קינה אחר</quote>,<title>elegy for the ninth of Ab</title>, beg.<quote type="beg">איך נוח שודד. ודודי עף ונודד</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_22-part1-item1-item2" n="b">
                           <p>(<locus from="4a" to="5a">fol. 4</locus>)<quote type="head">קינה אחרת</quote>, beg.<quote type="beg">אֿיכה יעיב לענות. בֿלע עשרים וארבע מתנות</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_22-part1-item1-item3" n="c">
                           <p>(<locus from="5b" to="8b">fol. 5b</locus>)<quote type="beg">איך בחרב ברוב כחשו</quote>.</p>
                        </msItem>
                        <note>At end (<locus from="7b" to="7b">fol. 7b</locus>)<quote type="own">הדה אלכראסה למנע (?) ואחוי עלי ותמים הרופאים יחיים אל בחסדו אמן סלה</quote>.</note>
                        <textLang mainLang="he" otherLangs="jrb"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. Rabb. char.; 8vo,<material>paper</material>.</p>
                  </physDesc>
                  <history>
                     <provenance>Owner:<persName type="heb">מנע (?) ואחוי עלי</persName>.</provenance>
                     <acquisition>Bought through the Rev.<persName>G. J. Chester</persName>,<date calendar="#Gregorian" when="1890">1890</date>(from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_f_22-part2" n="2">
