               <msPart xml:id="MS_Heb_d_19-part1" n="1">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 19/1</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_19-part1-item1">
                        <locus from="1a" to="5b"/>
                        <title>Mishnah</title>
                        <msItem xml:id="MS_Heb_d_19-part1-item1-item1" n="a">
                           <p>(<locus from="1a" to="2b">fol. 1</locus>)<title>ברכות</title>end of iv (המוספים) to end</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_19-part1-item1-item2" n="b">
                           <p>(<locus from="3a" to="5b">fol. 3</locus>)<title>פאה</title>to vi. 10. Arabic transl. of some words on the margins</p>
                        </msItem>
                        <note>At end of chapters הנא or הנה פירקא ; with superlinear vowel points.</note>
                        <textLang mainLang="he" otherLangs="jrb"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Yemen Rabb. char.; 4to,<material>vellum</material>, stained and injured.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev.<persName>G. J. Chester</persName>,<date calendar="#Gregorian" when="1890">1890</date>(from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_19-part2" n="2">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. d. 19/2</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_19-part2-item1">
                        <locus from="6a" to="7b"/>
                        <title>Mishnah</title>
                        <msItem xml:id="MS_Heb_d_19-part2-item1-item1" n="a">
                           <p>(<locus from="6a" to="6b">fol. 6</locus>)<title>ביצה</title>i. 6 (ביום טוב) to ii. 9 (MS. פרק א̇), incomplete</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_19-part2-item1-item2" n="b">
                           <p>(<locus from="7a" to="7b">fol. 7</locus>)<title>תענית</title>i. 7 (כבני אדם) to ii. 6 (וחכמים אומרין)</p>
                        </msItem>
                        <note>Some accents.</note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Span. squ. char.; 4to,<material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev.<persName>G. J. Chester</persName>,<date calendar="#Gregorian" when="1890">1890</date>(from the Genizah).</acquisition>
                  </history>
               </msPart>
