                  <msContents>
                     <msItem xml:id="MS_Heb_d_39-part1-item1">
                        <locus from="1a" to="1b"/>
                        <title>Genesis</title>
                        <note>xxvi. 11 to xxviii. 15, in shorthand form (see <ref target="MS_Heb_e_30_11">No. 2604, 11</ref>), with vowel points and accents.</note>
                        <textLang mainLang="he"/>
