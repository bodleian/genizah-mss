                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_e_33-part4-item1">
                        <locus from="32a" to="41b">Fol. 32</locus>
                        <title>Small pieces for פסח</title>
                        <note>beg. missing</note>
                        <msItem xml:id="MS_Heb_e_33-part4-item1-item1" n="1">
                           <p>headed<quote type="head">פזמון אל הים מפניהם</quote>, beg.<quote type="beg">לנבואה זימנתיך</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part4-item1-item2" n="2">
                           <p>(<locus from="32b" to="32b">fol. 32b</locus>) headed<quote type="head">ובכן היתה עלי יד ייי̇</quote>, beg.<quote type="beg">היתה עלי אימת צורי</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part4-item1-item3" n="3">
                           <p>(<locus from="33a" to="33a">fol. 33</locus>) headed<quote type="head">לחן</quote>, beg.<quote type="beg">אֿהוב נולד לשמח</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part4-item1-item4" n="4">
                           <p>(<locus from="33a" to="33b">ibid.</locus>) headings<quote type="head">ל̇ סיחת</quote>,<quote type="head">ל̇ אז מעשרים</quote>,<quote type="head">ונעריצך אלהינו</quote>, beg.<quote type="beg">אזכיר נעמיך</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part4-item1-item5" n="5">
                           <p>(<locus from="34a" to="34b">fol. 34</locus>) headings<quote type="head">שלשול ללכס</quote>,<quote type="head">יוצר לשבת וללכס</quote>,<quote type="head">יוצר לבפסח</quote>,</p>
                           <p>(<locus from="35a" to="38a">fol. 35</locus>)<quote type="head">פזמ̇ ליחיי</quote>, acr.<quote type="acr">יחיי חזק</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part4-item1-item6" n="6">
                           <p>(<locus from="38b" to="39a">fol. 38b</locus>) headed<quote type="head">פזמון</quote>, beg.<quote type="beg">מי כמוך דובר מישרים</quote>;</p>
                           <p>
                              <locus from="39b" to="39b">fol. 39b</locus>,<quote type="head">פזמ̇ לעמרם חזק</quote>, acr.<quote type="acr">עמרם חזק</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part4-item1-item7" n="7">
                           <p>(<locus from="40a" to="40b">fol. 40</locus>) headed<quote type="head">משה</quote>, beg.<quote type="beg">וארץ אמרה</quote>, incomplete;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part4-item1-item8" n="8">
                           <p>(<locus from="41a" to="41b">fol. 41</locus>) headings<quote type="head">פזמון לחן</quote>, beg.<quote type="beg">שלש חקות</quote>,<quote type="head">ל̇ מגביהי</quote>,<quote type="head">ל̇ הנה מטתו</quote>,<quote type="head">ל̇ תקיל</quote>,<quote type="head">מגייר ל̇ אמנם בעבור</quote>.</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
