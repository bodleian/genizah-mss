                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_e_33-part6-item1">
                        <locus from="65a" to="72b"/>
                        <title>Imprecations</title>
                        <msItem xml:id="MS_Heb_e_33-part6-item1-item1" n="1">
                           <p>(<locus from="65a" to="66b">fol. 65</locus>) beg. missing (?), beg.<quote type="beg">אין לי דורש למי אחו ואזכור קטב</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part6-item1-item2" n="2">
                           <p>(<locus from="67a" to="68b">fol. 67</locus>) headed<quote type="head">אחרת</quote>, beg.<quote type="beg">אליכם אשים אמימה (?) קולי</quote>, incomplete;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part6-item1-item3" n="3">
                           <p>(<locus from="69a" to="69a">fol. 69</locus>) end of a piece, beg.<quote type="beg">רובו דברך אשר למאד אחמדנו</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part6-item1-item4" n="4">
                           <p>(<locus from="69a" to="71a">ibid.</locus>) beg.<quote type="beg">אנה חי עולם ממני אל תתעלם</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part6-item1-item5" n="5">
                           <p>(<locus from="71b" to="72b">fol. 71b</locus>) headed<quote type="head">אחרת</quote>, beg.<quote type="beg">אחזתי בסאתי אשר הייתי מדד</quote>, incomplete.</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. curs. char.; 8vo,<material>paper</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev.<persName>G. J. Chester</persName>,<date calendar="#Gregorian" when="1891">1891</date>(from the Genizah).</acquisition>
                  </history>
