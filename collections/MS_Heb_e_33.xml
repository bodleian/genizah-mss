                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_e_33-part8-item1">
                        <locus from="74a" to="74b">Fol. 74</locus>
                        <title>End of a prayer, followed by הבדלה</title>
                        <msItem xml:id="MS_Heb_e_33-part8-item1-item1" n="1">
                           <p>beg.<quote type="beg">יה שלח מפנה מהלך לפניך</quote>, acr.<quote type="acr">יהוד</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part8-item1-item2" n="2">
                           <p>(<locus from="74b" to="74b">fol. 74b</locus>) headed<quote type="head">גירה לבן גיאת</quote>, beg.<quote type="beg">ירוצצו לגלות עוזך</quote>, acr.<quote type="acr">יצחק ג</quote>, incomplete.</p>
                        </msItem>
                        <textLang mainLang="he"/>
