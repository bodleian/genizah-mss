               <msPart xml:id="MS_Heb_e_33-part5" n="E">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. e. 33/E</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_e_33-part5-item1">
                        <locus from="42a" to="64b">Fol. 42</locus>
                        <title>סליחות</title>
                        <note>beg. missing, much injured</note>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item1" n="1">
                           <p>(<locus from="43a" to="43b">fol. 43</locus>) beg.<quote type="beg">אזי הייתי קלס לכל השאטים אותי</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item2" n="2">
                           <p>(<locus from="44a" to="45b">fol. 44</locus>) beg.<quote type="beg">את מי זנחתה לנצח כי מדוע זנחתנו לנצחים</quote>(cf.<locus from="25a" to="26a">fol. 25</locus>), א̇ב̇, acr.<quote type="acr">האיי</quote>(Zunz, p. 186);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item3" n="3">
                           <p>(<locus from="46a" to="47a">fol. 46</locus>) beg.<quote type="beg">אוי מי קשה כאב. אשר לבניו היה כאויב</quote>, (א̇ב̇), acr.<quote type="acr">עמרם</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item4" n="4">
                           <p>(<locus from="47b" to="48b">fol. 47b</locus>) beg.<quote type="beg">אכן סר מר המות. נמתק מדבש היטיב</quote>, (א̇ב̇),acr.<quote type="acr">האיי</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item5" n="5">
                           <p>(<locus from="49a" to="49b">fol. 49</locus>) beg.<quote type="beg">אין עמך לעצור. תהי ידך לעזרני</quote>, (א̇ב̇ to פ), incomplete;</p>
                           <p>
                              <locus from="50a" to="50b">fol. 50</locus>, א̇ב̇, beg. missing, beg. with מ,<quote type="beg">מדוע בשאלנו אין לנו חנות</quote>, acr. (so)<quote type="acr">האיי יניו</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item6" n="6">
                           <p>(<locus from="51a" to="55b">fol. 51</locus>)<quote type="beg">אויל עמי בי̇י למעול</quote>, (א̇ב̇, two strophes for each letter), incomplete;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item7" n="7">
                           <p>(<locus from="56a" to="56a">fol. 56</locus>) end of a piece, צ to ת, and acr. האיי;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item8" n="8">
                           <p>(<locus from="56a" to="57b">ibid.</locus>)<quote type="beg">אמר נא לכל הגויים</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item9" n="9">
                           <p>(<locus from="58a" to="58b">fol. 58</locus>)<quote type="beg">אנה עזב אל המון מעיו</quote>, (א̇ב̇), incomplete;</p>
                           <p>
                              <locus from="59a" to="59a">fol. 59</locus>, beg. missing, beg.<quote type="beg">הֿיה כאויב אל עליון. וֿיחמס כגן גיא חיזיון</quote>, (א̇ב̇), at the end<quote type="beg">ל̇ אם לא תאריך</quote>(cf.<locus from="25a" to="26a">fol. 25</locus>);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item10" n="10">
                           <p>(<locus from="59b" to="62a">fol. 59b</locus>)<quote type="beg">אני הגבר ראה כידו</quote>, (א̇ב̇ thrice);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item11" n="11">
                           <p>(<locus from="62b" to="63b">fol. 62b</locus>)<quote type="beg">איכה יועם זהם</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_33-part5-item1-item12" n="12">
                           <p>(<locus from="64a" to="64b">fol. 64</locus>)<quote type="beg">זכור יי מה היה לעם בחיר</quote>, incomplete.</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
