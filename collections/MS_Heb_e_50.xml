               <msPart xml:id="MS_Heb_e_50-part4" n="4">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. e. 50/4</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_e_50-part4-item1">
                        <locus from="27a" to="33b"/>
                        <title>סליחות</title>
                        <msItem xml:id="MS_Heb_e_50-part4-item1-item1" n="a">
                           <p>(<locus from="27a" to="28a">fol. 27</locus>) introduction (beg. missing);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_50-part4-item1-item2" n="b">
                           <p>(<locus from="28b" to="29a">fol. 28b</locus>) beg. <quote type="beg">אהה על רוב פשעי</quote> (חטאנו);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_50-part4-item1-item3" n="c">
                           <p>(<locus from="29b" to="30b">fol. 29b</locus>) <quote type="beg">תֿהלתך בשחרית הקשיבה</quote> (לשחרית), ת to ז;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_50-part4-item1-item4" n="d">
                           <p>(<locus from="31a" to="32a">fol. 31</locus>) <quote type="beg">איך אֿזלה ידי</quote> (למוסף), א̇ב̇;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_50-part4-item1-item5" n="e">
                           <p>(<locus from="32b" to="33a">fol. 32b</locus>) (למנחה) <quote type="beg">אפפוני חבלי זדוני</quote>, acr. <quote type="acr">אברהם</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_50-part4-item1-item6" n="f">
                           <p>(<locus from="33b" to="33b">fol. 33b</locus>) <quote type="beg">הרביתי לפשוע והשחתי</quote>, end missing.</p>
                        </msItem>
                        <note>With vowel points (Segol sometimes expressed by the sign of Pathah).</note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Yemen Rabb. char.; 8vo, <material>paper</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought from <persName>Wertheimer</persName>, <date calendar="#Gregorian" when="1893">1893</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
