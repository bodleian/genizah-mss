                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_d_32-part2-item1">
                        <locus from="3a" to="4b"/>
                        <title>A fragment of the Midrash on Proverbs</title>
                        <msItem xml:id="MS_Heb_d_32-part2-item1-item1" n="a">
                           <locus from="3a" to="3b">fol. 3</locus>
                           <title>Midrash on Prov. xvi.</title>
                           <note>xvi. 3 beg.<quote type="beg">מחשבותיו וליבו</quote>to xvi. 31<quote type="end">בדרך צדקה תמצא</quote>
                           </note>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_32-part2-item1-item2" n="b">
                           <locus from="4a" to="4b">fol. 4</locus>
                           <title>Midrash on Prov. xxiii to xxv</title>
                           <note>xxiii. 23 beg.<quote type="beg">ומנין שחזרו</quote>to xxv. 1<quote type="end">במסכיות</quote>.</note>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. squ. Rabb. char.; 8vo,<material>vellum</material>(<locus from="3a" to="3b">fol. 3</locus>injured).</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev.<persName>G. J. Chester</persName>,<date calendar="#Gregorian" when="1890">1890</date>(from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_d_32-part3" n="C">
                  <msIdentifier>
                     <altIdentifier type="partial">
