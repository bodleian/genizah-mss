                  <idno>MS. Heb. f. 39</idno><idno type="ieArk">ark:29072/x0dn39x201ns</idno><idno type="crArk">ark:29072/x0dj52w518b5</idno>
               </msIdentifier>
               <msContents>
                  <msItem xml:id="MS_Heb_f_39-item1">
                     <locus from="1a" to="94b"/>
                     <title>[יוצרות]</title>
                     <msItem xml:id="MS_Heb_f_39-item1-item1" n="1">
                        <p>(<locus from="1b" to="1b">fol. 1b</locus>) <quote type="head">ויהי מקץ</quote>, beg. and end wanting;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item2" n="2">
                        <p>(<locus from="2a" to="3a">fol. 2</locus>) <quote type="head">ויחי יעקב</quote>, beg. injured, beg. <quote type="beg">[אי]ש תם אחריתו להתעדנה בריתו לצוען</quote>;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item3" n="3">
                        <p>(<locus from="3b" to="8b">fol. 3b</locus>) <quote type="head">ספר אלה שמות</quote>, beg. <quote type="beg">אֿשר על כנפי נשרים נשואים. בֿרוכים</quote>, with <quote type="head">מאורות</quote> and <quote type="head">זולת</quote>, injured and much stained;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item4" n="4">
                        <p>(<locus from="9a" to="15b">fol. 9</locus>) <quote type="head">ואירא</quote>, beg. <quote type="beg">אֿשר ברשעו לבו הקשה</quote>, injured and stained; there are pieces headed <quote type="head">רשות</quote> and <quote type="head">מאורות</quote>;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item5" n="5">
                        <locus from="16a" to="21b">(fol. 16)</locus>
                        <quote type="head">ויהי בשלח</quote>
                        <msItem xml:id="MS_Heb_f_39-item1-item5-item1" n="a">
                           <p>
                              <quote type="head">רשות</quote>, beg. <quote type="beg">יאורו לי כפץ דרכו</quote>, acr. <quote type="acr">יהודה</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_39-item1-item5-item2" n="b">
                           <p>
                              <quote type="head">יוצר</quote>, beg. <quote type="beg">אויב בכעס הוזעם</quote>;</p>
                           <p>
                              <locus from="17b" to="20a">fol. 17b</locus>, <quote type="head">אופן</quote>, beg. <quote type="beg">אודה לשכון שמי עליה</quote>;</p>
                           <p>
                              <locus from="20b" to="21a">fol. 20b</locus>, <quote type="head">זולת</quote>, beg. <quote type="beg">אדון אשר מכל עין נעלם</quote>, and <quote type="head">זולת אחור</quote>, beg. <quote type="head">לגוזיר אדירי ימים הנעימו שבח</quote>, (together א̇ב̇);</p>
                           <p>
                              <locus from="21b" to="21b">fol. 21b</locus>, <quote type="head">יאורו לי</quote> (as on <locus from="16a" to="16a">fol. 16</locus>), followed by <quote type="beg">לכשף אביל דרכיו</quote>, and some Biblical passages with the direction, <quote>יקול הדא אלפסוק ג̇ רפעאת</quote>;</p>
                        </msItem>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item6" n="6">
                        <p>(<locus from="22a" to="25b">fol. 22</locus>) <quote type="head">יתרו</quote>, beg. missing, with (<locus from="23b" to="24a">fol. 23b</locus>) <quote type="head">מאורות</quote>, beg. <quote type="beg">שׂם גוי אחד בארץ</quote>, and (<locus from="24b" to="25b">fol. 24b</locus>) <quote type="head">זולת</quote>, beg. <quote type="beg">אחד מי אלוף</quote>, (א̇ב̇);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item7" n="7">
                        <p>(<locus from="26a" to="26b">fol. 26</locus>) <quote type="head">צו</quote>, beg. <quote type="beg">אדון צרור המור</quote>, (א̇ב̇ to פ);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item8" n="8">
                        <p>(<locus from="27a" to="28b">fol. 27</locus>) <quote type="head">ויהי ביום השמיני</quote>, beg. <quote type="beg">אמוניך כיום שנטל עשר עטרות</quote>;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item9" n="9">
                        <locus from="29a" to="30b">(fol. 29)</locus>
                        <quote type="head">אשה וזאת</quote>
                        <msItem xml:id="MS_Heb_f_39-item1-item9-item1" n="a">
                           <p>
                              <quote type="head">יוצר</quote>, beg. <quote type="beg">אשה אשר היריון לה יארע</quote>, (א̇ב̇ to כ);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_39-item1-item9-item2" n="b">
                           <p>(<locus from="31a" to="31a">fol. 31</locus>) <quote type="head">יוצר</quote>, beg. <quote type="beg">אמרי דת לנצור ולשמור</quote>, (א̇ב̇ to ט);</p>
                        </msItem>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item10" n="10">
                        <locus from="31b" to="32b">(fol. 31b)</locus>
                        <quote type="head">וזאת תהיה</quote>
                        <msItem xml:id="MS_Heb_f_39-item1-item10-item1" n="a">
                           <p>
                              <quote type="head">יוצר</quote>, beg. <quote type="beg">אֿמרים קלים וחמודים אותם לגמור</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_39-item1-item10-item2" n="b">
                           <p>b (<locus from="32b" to="32b">fol. 32b</locus>) <quote type="head">יוצר אחרי מות וקדושים</quote>, beg. <quote type="beg">וידבר יי̇ אל משה גילה לכהניי</quote>;</p>
                        </msItem>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item11" n="11">
                        <p>(<locus from="33a" to="34a">fol. 33</locus>) <quote type="head">אחרי מות</quote>, beg. <quote type="beg">אכפיל עווניי באספת שני</quote>, (א̇ב̇);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item12" n="12">
                        <p>(<locus from="33b" to="34a">fol. 33b</locus>) beg. <quote type="beg">אמונים אשר עלו חמושי</quote>, (א̇ב̇);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item13" n="13">
                        <p>(<locus from="34b" to="36a">fol. 34b</locus>) <quote type="head">בהר סני ואם בחקתי</quote>, beg. <quote type="beg">אתם היום תאריכו</quote>;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item14" n="14">
                        <p>(<locus from="36b" to="37a">fol. 36b</locus>) <quote type="head">אם בחקותי</quote>, beg. <quote type="beg">אֿזנכם הטו ואלי לכו. בֿקומה</quote>, followed by a short <quote type="head">מאורה</quote>;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item15" n="15">
                        <p>(<locus from="37b" to="39a">fol. 37b</locus>) <quote type="head">ספר וידבר</quote>, beg. <quote type="beg">אהבים בצ[א]תם מארץ כושה</quote>, and <quote type="beg">אדון שח לרועה אמוניי</quote>, (א̇ב̇), headed <quote type="head">יוצר אחיר</quote>;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item16" n="16">
                        <p>(<locus from="39b" to="41b">fol. 39b</locus>) <quote type="head">נשוא</quote>, <quote type="head">יוצר</quote>, beg. <quote type="beg">אל אשר מעולם מחשה</quote>, (א̇ב̇), much stained;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item17" n="17">
                        <p>(<locus from="42a" to="45b">fol. 42</locus>) <quote type="head">בהעלותך</quote>, beg. injured;</p>
                        <p>
                           <locus from="44a" to="45b">fol. 44</locus>, another beg. <quote type="beg">אומץ ניר מנורת מחשה</quote>, (א̇ב̇);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item18" n="18">
                        <p>(<locus from="46a" to="47a">fol. 46</locus>) <quote type="head">שלח לך</quote>, beg. <quote type="beg">אמונים ליסע לישע בהר המור</quote>, (א̇ב̇);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item19" n="19">
                        <p>(<locus from="47b" to="50b">fol. 47b</locus>) <quote type="head">ויקח קרח</quote>, beg. <quote type="beg">אנשי לצון ברשע מיוהר</quote>, (א̇ב̇);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item20" n="20">
                        <p>(<locus from="51a" to="51b">fol. 51</locus>) <quote type="head">זאת חקת התורה</quote>, beg. <quote type="beg">אל נערץ בסוד קדושי</quote>, (א̇ב̇);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item21" n="21">
                        <p>(<locus from="52a" to="53b">fol. 52</locus>) <quote type="head">וירא בלק</quote>, beg. <quote type="beg">אשר הודו שמים כסה</quote>, two pieces with the same beginning, interrupted by</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item22" n="22">
                        <p>(<locus from="54a" to="55b">fol. 54</locus>) <quote type="head">רשות לאביל</quote>, beg. <quote type="beg">הכל יפארוך אלהי עולם</quote>, incomplete;</p>
                        <p>
                           <locus from="55a" to="55b">fol. 55</locus>, continuation of <ref target="#MS_Heb_f_39_21">No. 21</ref>, followed by two <quote type="head">מאורות</quote>, incomplete;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item23" n="23">
                        <p>(<locus from="56a" to="56a">fol. 56</locus>) beg. missing, end <quote type="end">ובאו ציון ברנה ונשיר לו שירים</quote>, (for אבל?);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item24" n="24">
                        <p>(<locus from="56a" to="57a">ibid.</locus>) <quote type="head">יוצר מילה</quote>, beg. <quote type="beg">אדון שח לרועה איומת</quote> ו, (א̇ב̇);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item25" n="25">
                        <p>(<locus from="57b" to="58b">fol. 57b</locus>) <quote type="head">יוצר שבת</quote>, beg. <quote type="beg">שיר השירים אצחציח</quote>, (א̇ב̇);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item26" n="26">
                        <p>(<locus from="59a" to="59b">fol. 59</locus>) <quote type="head">יוצר בראשית</quote>, beg. <quote type="beg">אכתיר פה למלך דברו</quote>, (א̇ב̇), incomplete;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item27" n="27">
                        <p>(<locus from="60a" to="60b">fol. 60</locus>) <quote type="head">יוצר ללדת בן</quote>, beg. <quote type="beg">אזכיר חסדי יי̇ אשר גמלנו</quote>, (א̇ב̇);</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item28" n="28">
                        <locus from="61a" to="85b">(fol. 61)</locus>
                        <quote type="head">אביל</quote>
                        <msItem xml:id="MS_Heb_f_39-item1-item28-item1" n="a">
                           <p>
                              <quote type="head">רשות ליוצר</quote>, beg. <quote type="beg">ויהי הכל במאמר אלאלים</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_39-item1-item28-item2" n="b">
                           <p>(<locus from="63a" to="64a">fol. 63</locus>) <quote type="head">יוצר</quote>, beg. <quote type="beg">אנוש רימה ובן אדם תולעה</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_39-item1-item28-item3" n="c">
                           <p>(<locus from="64b" to="65b">fol. 64b</locus>) <quote type="head">סלוק</quote>, beg. <quote type="beg">העת ישקיף דודי צח</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_39-item1-item28-item4" n="d">
                           <p>(<locus from="66a" to="72b">fol. 66</locus>) <quote type="head">אופן</quote>, beg. <quote type="beg">אל השמים אל מעל יקרא</quote>, (א̇ב̇);</p>
                           <p>
                              <locus from="67a" to="69b">fol. 67</locus>, <quote type="head">מאורות</quote>, beg. <quote type="beg">ארומים אל ואגדלו</quote>;</p>
                           <p>
                              <locus from="70a" to="72b">fol. 70</locus>, another <quote type="head">אופן</quote>, beg. <quote type="beg">אין לפני דיין משוא פנים</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_39-item1-item28-item5" n="e">
                           <p>(<locus from="73a" to="76b">fol. 73</locus>) <quote type="head">זולת</quote>, beg. <quote type="beg">אמת תעוריר רדומים לנחים עמי</quote>;</p>
                           <p>
                              <locus from="75a" to="75b">fol. 75</locus>, <quote type="head">מי כמוך</quote>, beg. <quote type="beg">שוטיח עלי חוזך עננת ערפל</quote>; <quote type="head">יי̇ מלכינו</quote>, beg. <quote type="beg">לך קוינו מהר חיש</quote>, acr. <quote type="acr">שלמה</quote>;</p>
                           <p>
                              <locus from="76a" to="76b">fol. 76</locus>, another <quote type="head">זולת</quote>, beg. <quote type="beg">אנוש יצא מביתו ואוהלו</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_39-item1-item28-item6" n="f">
                           <p>(<locus from="77a" to="81b">fol. 77</locus>) <quote type="head">מי כמוך</quote>, beg. <quote type="beg">מהולל ומשבח בתחת ובמעל</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_39-item1-item28-item7" n="g">
                           <p>(<locus from="82a" to="85b">fol. 82</locus>) <quote type="head">זולת</quote>, beg. <quote type="beg">אין זולתו מעלינו אמוניו</quote>;</p>
                        </msItem>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item29" n="29">
                        <p>(<locus from="86a" to="92b">fol. 86</locus>) <quote type="head">רשות חתן</quote>, beg. <quote type="beg">ישמח יי̇ במעשה ידיו</quote>, (א̇ב̇), acr. <quote type="acr">יוסף יזכה</quote>, followed by <quote type="head">מאורות</quote>, <quote type="head">זולת</quote>, <quote type="head">פזמון</quote>, and <quote type="head">אופן</quote>;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item30" n="30">
                        <p>(<locus from="93a" to="94b">fol. 93</locus>) <quote type="head">יוצר</quote> for <quote>חיי שרה</quote>, beg. <quote type="beg">אשת חייל אשרתה</quote>.</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item31">
                        <p>Ff. <locus from="4b" to="4b">4b</locus> and <locus from="12b" to="12b">12b</locus> contain fragments of an <title>Arabic sermon (?)</title>, injured and stained;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item32">
                        <p>ff. <locus from="29b" to="29b">29b</locus> and <locus from="30a" to="30a">30a</locus>, <title>a Hebrew letter</title>, of no historical importance, addressed to <persName type="ment" key="person_1967">Abraham חמודינו</persName>, dated <date calendar="#Hebrew-Seleucid" when="1354">Marḥeshwan 1354 Sel.</date> = <date calendar="#Gregorian" when="1043">1043</date>;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item33">
                        <p>fol. <locus from="45b" to="45b">45b</locus> has the name <persName type="ment" key="person_1968">Jacob hak-Kohen</persName>;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item34">
                        <p>ff. <locus from="68b" to="68b">68b</locus>, <locus from="69a" to="69a">69a</locus>, <title>an Arabic contract</title> between <persName type="ment" key="person_836">Isaac</persName> and <persName type="ment" key="person_1969">Jacob hak-Kohen, son of Aaron</persName>, dated <date calendar="#Hebrew-Seleucid" when="1320">2nd of Sivan [13]20</date>, <quote>בעי[ר פסטאט] דעל נילוס נהרא מותבה</quote>, injured;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item35">
                        <p>ff. <locus from="82b" to="82b">82b</locus> and <locus from="83a" to="83a">83a</locus>, <title>fragment of an Arabic document</title>, much cut, the name of <persName type="ment" key="person_1970">Sheikh Abu-l-Karim</persName> occurs in it;</p>
                     </msItem>
                     <msItem xml:id="MS_Heb_f_39-item1-item36">
                        <p>fol. <locus from="87b" to="87b">87b</locus>, <title>small fragment of an Arabic letter</title>, in which the name ' <persName key="person_1971">my brother Jacob</persName>' occurs.</p>
                     </msItem>
                     <textLang mainLang="he" otherLangs="jrb"/>
                  </msItem>
               </msContents>
               <physDesc>
                  <p>Syr. Rabb. char.; small 8vo, <material>paper</material>, much injured and stained.</p>
                  <p>Ff. 94.</p>
               </physDesc>
