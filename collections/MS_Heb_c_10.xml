                     <msItem xml:id="MS_Heb_c_10-part1-item1">
                        <locus from="1a" to="1b">Fol. 1</locus>
                        <title>Rashi's commentary on Babylonian Talmud תענית</title>
                        <note>fol. 21ab.</note>
                        <textLang mainLang="he"/>
