               <msPart xml:id="MS_Heb_f_30-part1" n="1">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. f. 30/1</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_f_30-part1-item1">
                        <locus from="1a" to="7b"/>
                        <title>The prayer <quote type="head">יי אלהי ישראל</quote>, and the liturgical pieces beg. <quote type="beg">אֿביוני עמך</quote>
                        </title>
                        <note>with Pizmonim (<locus from="2a" to="2b">fol. 2</locus>) <quote type="beg">אם עונינו</quote>; <quote type="beg">אתאנו לחלות פניך</quote>; <p>
                              <locus from="3a" to="3b">fol. 3</locus>, <quote type="beg">קראנוך אלהינו</quote>;</p>
                           <p>
                              <locus from="4a" to="7b">fol. 4</locus>, <quote type="beg">בשרתי צדק</quote>; <quote type="beg">לילה להודות לך</quote>.</p>
                        </note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. Rabb. squ. char.; small 8vo, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName key="person_744">G. J. Chester</persName>, <date calendar="#Gregorian" when="1891">1891</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_f_30-part2" n="2">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. f. 30/2</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_f_30-part2-item1">
                        <locus from="8a" to="11b"/>
                        <title>Liturgical</title>
                        <msItem xml:id="MS_Heb_f_30-part2-item1-item1" n="a">
                           <locus from="8a" to="10b">fol. 8</locus>
                           <title>introductory pieces to Seliḥoth</title>
                           <note>, e.g. <quote type="beg">אם הרבינו אשמה</quote>; <quote type="beg">אם רבו חטאי</quote>; <p>
                                 <locus from="9b" to="9b">fol. 9b</locus>, <quote type="beg">עד מתי באשש</quote>;</p>
                              <p>
                                 <locus from="10a" to="10b">fol. 10</locus>, <quote type="beg">המכין לעורב צידו</quote>, headed <quote type="head">פזמון</quote>;</p>
                           </note>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part2-item1-item2" n="b">
                           <p>(<locus from="11a" to="11b">fol. 11</locus>) א̇ב̇, beg. and end missing, ד beg. as follows, <quote>דאובים מרוע מעללם. ההלכים לאמור בגללם. עזבו נתיבות און ושבילם</quote>.</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. squ. char.; small 8vo, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName key="person_744">G. J. Chester</persName>, <date calendar="#Gregorian" when="1891">1891</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_f_30-part3" n="3">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. f. 30/3</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_f_30-part3-item1">
                        <locus from="12a" to="17b">Fol. 12</locus>
                        <title>Liturgical</title>
                        <note>Much obliterated;</note>
                        <msItem xml:id="MS_Heb_f_30-part3-item1-item1">
                           <p>
                              <locus from="13a" to="14b">fol. 13</locus>, <quote type="beg">הלנצח תאחר המעמד</quote>, at end <quote type="head">סליחה לעשרה בטבת</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part3-item1-item2">
                           <p>
                              <locus from="15a" to="15b">fol. 15</locus>, <quote type="beg">אֿמיץ כח שוכן שמי שבעה בך אבטחה</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part3-item1-item3">
                           <p>
                              <locus from="16a" to="17b">fol. 16</locus>, <quote type="beg">אֿזון שועתינו האל המרחם</quote>.</p>
                        </msItem>
                        <note>On <locus from="14b" to="14b">fol. 14b</locus>, <quote type="own">הדא דפתר לשמואל</quote>.</note>
                        <textLang mainLang="he" otherLangs="jrb"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. Rabb. char.; small 8vo, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <provenance>Owner: <persName type="own" key="person_180">Samuel</persName>.</provenance>
                     <acquisition>Bought through the Rev. <persName key="person_744">G. J. Chester</persName>, <date calendar="#Gregorian" when="1891">1891</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_f_30-part4" n="4">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. f. 30/4</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_f_30-part4-item1">
                        <locus from="18b" to="21b">Fol. 18b</locus>
                        <title>Introduction to Seliḥoth</title>
                        <note>beg. <quote type="beg">שימה דימעתינו בנאדך</quote>, followed by <quote type="beg">אם אֿשמינו כתולע</quote>; <p>
                              <locus from="19b" to="20b">fol. 19b</locus>, <quote type="beg">אֿערוך חין שועי</quote>;</p>
                           <p>
                              <locus from="21a" to="21b">fol. 21</locus>, <quote type="beg">אֿנשי אמונה נעלמו</quote> (Zunz, p. 220).</p>
                        </note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. Rabb. char.; small 8vo, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName key="person_744">G. J. Chester</persName>, <date calendar="#Gregorian" when="1891">1891</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_f_30-part5" n="5">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. f. 30/5</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_f_30-part5-item1">
                        <locus from="22a" to="29b">Fol. 22</locus>
                        <title>Pieces for תחנון and other occasions, obliterated;</title>
                        <note>
                           <p>
                              <locus from="23b" to="24a">fol. 23b</locus>, headed <quote type="head">יי̇ אלהי ישראל לעצירה</quote>, beg. <quote type="beg">עד מתי אנחות</quote>;</p>
                           <p>
                              <locus from="24b" to="26a">fol. 24b</locus>, <quote type="beg">תפלה לעני כי יעטוף נכחך בהגיון</quote>;</p>
                           <p>
                              <locus from="26b" to="29b">fol. 26b</locus>, headed <quote type="head">י̇ז̇ בתמוז</quote>, beg. <quote type="beg">עד מתי בכל שנה</quote>.</p>
                        </note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. curs. char.; small 8vo, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName key="person_744">G. J. Chester</persName>, <date calendar="#Gregorian" when="1891">1891</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_f_30-part6" n="6">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. f. 30/6</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_f_30-part6-item1">
                        <locus from="30a" to="37b"/>
                        <title>רהט</title>
                        <msItem xml:id="MS_Heb_f_30-part6-item1-item1" n="a">
                           <p>(<locus from="30b" to="31a">fol. 30b</locus>) <quote type="beg">אלי יכונן מקום זבולי</quote>, acr. <quote type="acr">אברהם בר החזן</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part6-item1-item2" n="b">
                           <p>(<locus from="31b" to="32b">fol. 31b</locus>) <quote type="beg">מילה מבורכה עשו כהלכה</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part6-item1-item3" n="c">
                           <p>(<locus from="33a" to="33a">fol. 33</locus>) <quote type="beg">ענני אלהי צדקי</quote>, acr. <quote type="acr">עמרם חזן</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part6-item1-item4" n="d">
                           <p>(<locus from="33b" to="34b">fol. 33b</locus>) <quote type="beg">יגל כבודי בישעך</quote>, acr. <quote type="acr">יחזקאל הכהן ינן</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part6-item1-item5" n="e">
                           <p>(<locus from="35a" to="36a">fol. 35</locus>) <quote type="beg">יושב תהלות חזה</quote>, acr. <quote type="acr">יושף ב</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part6-item1-item6" n="f">
                           <p>(<locus from="36b" to="36b">fol. 36b</locus>) <quote type="beg">בקראי לך צור ענני</quote>, acr. <quote type="acr">בשר בן יצחק חזן</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part6-item1-item7" n="f">
                           <p>(<locus from="37a" to="37b">fol. 37</locus>) <quote type="beg">ירד עלה סיני</quote>, acr. <quote type="acr">יושף אלברד (?)</quote>.</p>
                        </msItem>
                        <note>With vowel points.</note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. squ. char.; small 8vo, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName key="person_744">G. J. Chester</persName>, <date calendar="#Gregorian" when="1891">1891</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_f_30-part7" n="7">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. f. 30/7</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_f_30-part7-item1">
                        <locus from="38a" to="47b"/>
                        <title>Liturgical</title>
                        <msItem xml:id="MS_Heb_f_30-part7-item1-item1" n="a">
                           <p>(<locus from="38a" to="41b">fol. 38</locus>) <title>fragment of common prayers</title>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part7-item1-item2" n="b">
                           <p>(<locus from="42a" to="43a">fol. 42</locus>) headed <quote type="head">בשם יוצר הכל י̇ח̇ לחדש וחנוכה</quote>, beg. <quote type="beg">יהיה אדון לנו בכל דור מושיעי</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part7-item1-item3" n="c">
                           <p>(<locus from="43b" to="46b">fol. 43b</locus>) headed <quote type="head">י̇ח̇ ללפסח</quote>, beg. <quote type="beg">ויושע אדון אמוני ארץ</quote>,</p>
                           <p>
                              <locus from="44b" to="46b">fol. 44b</locus>, <quote type="beg">אֿיום בקרעך גזרים</quote>.</p>
                        </msItem>
                        <note>
                           <p>Colophon, (<locus from="47a" to="47b">fol. 47</locus>) <quote type="col">צמח החזן ביר̇ ינון הדין כתב ברחבה באב שנת א̇ש̇מ̇ לשלמה בר יצחק יחיה לעד</quote>.</p>
                           <p>Written by <persName type="scribe" key="person_1064">Ẓemaḥ bar Yinon</persName> for <persName type="own" key="person_1065">Sh'lomoh bar Yiẓḥaq</persName> in the month of <date calendar="#Hebrew-Seleucid" when="1340">Ab 1340</date> = <date calendar="#Gregorian" when="1029">1029 A.D.</date>
                           </p>
                        </note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. Rabb. char.; small 8vo, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <origin>
                        <origDate when="1029">1029</origDate>
                        <p>Scribe: <persName type="scribe" key="person_1066">Ẓemaḥ (ḥazan) bar Yinon</persName>.</p>
                        <p>Owner: <persName type="own" key="person_1067">Solomon b. Isaac</persName>.</p>
                     </origin>
                     <acquisition>Bought through the Rev. <persName key="person_744">G. J. Chester</persName>, <date calendar="#Gregorian" when="1891">1891</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_f_30-part8" n="8">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. f. 30/8</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_f_30-part8-item1">
                        <locus from="48a" to="53b"/>
                        <title>Liturgical</title>
                        <msItem xml:id="MS_Heb_f_30-part8-item1-item1" n="a">
                           <p>(<locus from="48a" to="48a">fol. 48</locus>) obliterated;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part8-item1-item2" n="b">
                           <p>(<locus from="48b" to="48b">fol. 48b</locus>) <quote type="beg">עד מתי תאחר הזמן עשות נקמה</quote>, headed <quote type="head">פֿיֿזֿ</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part8-item1-item3" n="c">
                           <p>(<locus from="49a" to="49a">fol. 49</locus>) <quote type="beg">אבירי לב משמע</quote>, (א̇ב̇ to י);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part8-item1-item4" n="d">
                           <p>(<locus from="49b" to="49b">fol. 49b</locus>) <quote type="beg">אשנסה מתני במחגרת פחד</quote>, (א̇ב̇ to ח);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part8-item1-item5" n="e">
                           <p>(<locus from="50a" to="50b">fol. 50</locus>) <quote type="beg">אקשטה כסל וקרב</quote>, incomplete, ends with ה (Zunz, p. 229, 83); the last three are headed <quote type="head">שמע ישראל</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part8-item1-item6" n="f">
                           <p>(<locus from="51a" to="51a">fol. 51</locus>) <quote type="beg">ארומם ואשבח לצור במען</quote>, (א̇ב̇), headed <quote type="head">יוצר</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part8-item1-item7" n="g">
                           <p>(<locus from="51b" to="51b">fol. 51b</locus>) <quote type="beg">אמולים באחת ובעשר קמוטים</quote>, (א̇ב̇);</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part8-item1-item8" n="h">
                           <p>(<locus from="52a" to="52a">fol. 52</locus>) <quote type="beg">אערוג לך מחמדי</quote>, (א̇ב̇); the last two are headed <quote type="head">זולת</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part8-item1-item9" n="i">
                           <p>(<locus from="52b" to="52b">fol. 52b</locus>) <quote type="beg">קדושה לאל בנועם</quote>, headed <quote type="head">יי̇ מלכינו</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part8-item1-item10" n="k">
                           <p>(<locus from="53a" to="53b">fol. 53</locus>) beg. <quote type="beg">אֿ...ע קצת פי על שם צור עולמים</quote>, headed <quote type="head">סלוק</quote>.</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. squ. Rabb. char.; small 8vo, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName key="person_744">G. J. Chester</persName>, <date calendar="#Gregorian" when="1891">1891</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_f_30-part9" n="9">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. f. 30/9</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_f_30-part9-item1">
                        <locus from="54a" to="60b">Fol. 54</locus>
                        <title>Fragment of a Siddur, containing the prayers for Sukkoth</title>
                        <note>with (<locus from="55b" to="56b">fol. 55b</locus>) <quote type="head">אכתיר</quote>, (<locus from="57a" to="60b">fol. 57</locus>) <quote type="beg">אאמר אותך בהוד והדר</quote>, and <quote type="head">אנא</quote> (Zunz, p. 70).</note>
                        <textLang mainLang="he"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. Rabb. char.; small 8vo, <material>vellum</material>.</p>
                  </physDesc>
                  <history>
                     <acquisition>Bought through the Rev. <persName key="person_744">G. J. Chester</persName>, <date calendar="#Gregorian" when="1891">1891</date> (from the Genizah).</acquisition>
                  </history>
               </msPart>
               <msPart xml:id="MS_Heb_f_30-part10" n="10">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Heb. f. 30/10</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_f_30-part10-item1">
                        <locus from="61a" to="73b"/>
                        <title>Liturgical</title>
                        <msItem xml:id="MS_Heb_f_30-part10-item1-item1" n="a">
                           <p>(<locus from="61b" to="66a">fol. 61b</locus>) <quote type="beg">ויהי שָ... ראשית כל מלאכה</quote>, (ש̇ע̇), headed <quote type="head">ז̇ ל[שבת וחנוכה]</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part10-item1-item2" n="b">
                           <p>(<locus from="66b" to="67b">fol. 66b</locus>) <quote type="beg">אצילי עדת מי מנה</quote>, headed <quote type="head">מעריב שבת וח̇ חנוכה</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part10-item1-item3" n="c">
                           <p>(<locus from="68a" to="71a">fol. 68</locus>) <quote type="beg">זומנו שיר שבת</quote>, headed <quote type="head">זולת</quote>;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_f_30-part10-item1-item4" n="d">
                           <p>(<locus from="71b" to="73b">fol. 71b</locus>) <quote type="beg">ויגש אליו אריה</quote>, headed <quote type="head">ויגש אליו</quote>.</p>
                        </msItem>
                        <textLang mainLang="he"/>
                     </msItem>
