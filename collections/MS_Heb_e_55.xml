                     <altIdentifier type="partial">
                        <idno>MS. Heb. e. 55/4</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Heb_e_55-part4-item1">
                        <locus from="11b" to="12b"/>
                        <title>Liturgical</title>
                        <msItem xml:id="MS_Heb_e_55-part4-item1-item1" n="a">
                           <p>(<locus from="11b" to="11b">fol. 11b</locus>) <quote type="beg">ח̇ גבעה מעולה למאד גואל יבשר גילו מאד</quote>, (א̇ב̇)</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_55-part4-item1-item2" n="b">
                           <p>(<locus from="11b" to="11b">ibid.</locus>) beg. (injured) <quote type="beg">ובכן ואז̇ בצאן קדשים</quote> (four lines)</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_e_55-part4-item1-item3" n="c">
                           <p>(<locus from="12a" to="12b">fol.12</locus>) beg. <quote type="beg">ארשה בשפתי ואפתח במילי ואסלד לכוכב בערבות</quote>, followed by an Arabic translation, beg. <quote type="beg">אסתאדן בנטקי ואפתתח בכלאמי</quote>
                           </p>
                        </msItem>
                        <textLang mainLang="he" otherLangs="jrb"/>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <p>Syr. Rabb. char.; 4to, <material>vellum</material>.</p>
                  </physDesc>
