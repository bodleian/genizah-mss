                  <msContents>
                     <msItem xml:id="MS_Heb_d_33-part8-item1">
                        <locus from="18a" to="19b"/>
                        <title>Grammatical fragment in Arabic</title>
                        <msItem xml:id="MS_Heb_d_33-part8-item1-item1" n="a">
                           <p>(<locus from="18a">fol. 18</locus>) on verbs ל''ה;</p>
                        </msItem>
                        <msItem xml:id="MS_Heb_d_33-part8-item1-item2" n="b">
                           <p>(<locus from="19a">fol. 19</locus>) on Hithpael and Hiphil forms, and quadrilateral roots.</p>
                        </msItem>
                        <textLang mainLang="jrb"/>
