                  <msContents>
                     <msItem xml:id="MS_Heb_d_33-part5-item1">
                        <locus from="12a" to="13b">Fol. 12</locus>
                        <title>A list of the minor lessons read on Mondays and Thursdays</title>
                        <note>beg. Numbers i. 1 to Deuteronomy xxxii. 18.</note>
                        <textLang mainLang="he"/>
                     </msItem>
