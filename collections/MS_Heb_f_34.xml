                     <msItem xml:id="MS_Heb_f_34-part5-item1">
                        <locus from="10a" to="20b">Fols. 10-20</locus>
                        <title>Fragment of a סדור</title>
                        <note>beg. with <quote type="beg">והוא רחום</quote>, headed <quote type="head">למעריב</quote>, ending with <quote type="end">תפילה לכיפור</quote>, incomplete.</note>
                        <textLang mainLang="he"/>
                     </msItem>
